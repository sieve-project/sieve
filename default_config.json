{
    "docker_registry": "ghcr.io/sieve-project/action",
    "controller_folder": "examples",
    "namespace": "default",
    "leading_api": "kind-control-plane",
    "following_api": "kind-control-plane3",
    "state_update_summary_check_enabled": true,
    "end_state_check_enabled": true,
    "workload_error_check_enabled": true,
    "controller_exception_check_enabled": true,
    "state_update_summary_check_event_list": [
        "DELETED",
        "ADDED"
    ],
    "compress_trivial_reconcile_enabled": false,
    "workload_hard_timeout": 600,
    "workload_soft_timeout": 100,
    "generate_debugging_information_enabled": true,
    "causality_prunning_enabled": true,
    "effective_updates_pruning_enabled": true,
    "nondeterministic_pruning_enabled": true,
    "persist_test_plans_enabled": true,
    "field_key_mask": {
        "*/*/*": [
            [
                "image"
            ],
            [
                "imageID"
            ],
            [
                "containerID"
            ],
            [
                "uid"
            ],
            [
                "resourceVersion"
            ]
        ]
    },
    "field_path_mask": {
        "*/*/*": [
            [
                "metadata",
                "annotations"
            ],
            [
                "metadata",
                "managedFields"
            ],
            [
                "metadata",
                "generateName"
            ],
            [
                "metadata",
                "generation"
            ],
            [
                "metadata",
                "deletionGracePeriodSeconds"
            ],
            [
                "status",
                "conditions"
            ],
            [
                "status",
                "observedGeneration"
            ]
        ],
        "pod/*/*": [
            [
                "spec",
                "nodeName"
            ]
        ],
        "controllerrevision/*/*": [
            [
                "spec",
                "ports"
            ]
        ],
        "replicaset/*/*": [
            [
                "spec",
                "selector",
                "pod-template-hash"
            ]
        ],
        "endpoints/*/*": [
            [
                "subsets"
            ]
        ],
        "lease/*/*-leader-election": [
            [
                "spec",
                "leaseTransitions"
            ]
        ]
    },
    "state_update_summary_checker_mask": [
        "endpoints/*/*-metrics",
        "endpointslice/*/*-metrics-*"
    ]
}